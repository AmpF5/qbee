{
  "name": "qbee",
  "displayName": "QBee",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qbee.registerBookmark1",
        "title": "Register Bookmark 1"
      },
      {
        "command": "qbee.registerBookmark2",
        "title": "Register Bookmark 2"
      },
      {
        "command": "qbee.registerBookmark3",
        "title": "Register Bookmark 3"
      },
      {
        "command": "qbee.registerBookmark4",
        "title": "Register Bookmark 4"
      },
      {
        "command": "qbee.registerBookmark5",
        "title": "Register Bookmark 5"
      },
      {
        "command": "qbee.jumpBookmark1",
        "title": "Jump to Bookmark 1"
      },
      {
        "command": "qbee.jumpBookmark2",
        "title": "Jump to Bookmark 2"
      },
      {
        "command": "qbee.jumpBookmark3",
        "title": "Jump to Bookmark 3"
      },
      {
        "command": "qbee.jumpBookmark 4",
        "title": "Jump to Bookmark 4"
      },
      {
        "command": "qbee.jumpBookmark5",
        "title": "Jump to Bookmark 5"
      }
    ],
    "keybindings":[
      {
        "command": "qbee.registerBookmark1",
        "key": "alt+shift+1",
        "when": "editorTextFocus"
      },
      {
        "command": "qbee.registerBookmark2",
        "key": "alt+shift+2",
        "when": "editorTextFocus"
      },
      {
        "command": "qbee.registerBookmark3",
        "key": "alt+shift+3",
        "when": "editorTextFocus"
      },
      {
        "command": "qbee.registerBookmark4",
        "key": "alt+shift+4",
        "when": "editorTextFocus"
      },
      {
        "command": "qbee.registerBookmark5",
        "key": "alt+shift+5",
        "when": "editorTextFocus"
      },
      {
        "command": "qbee.jumpBookmark1",
        "key": "alt+1",
        "when": "editorTextFocus"
      },
      {
        "command": "qbee.jumpBookmark2",
        "key": "alt+2",
        "when": "editorTextFocus"
      },
      {
        "command": "qbee.jumpBookmark3",
        "key": "alt+3",
        "when": "editorTextFocus"
      },
      {
        "command": "qbee.jumpBookmark4",
        "key": "alt+4",
        "when": "editorTextFocus"
      },
      {
        "command": "qbee.jumpBookmark5",
        "key": "alt+5",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
